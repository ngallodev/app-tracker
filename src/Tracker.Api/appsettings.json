{
  "Llm": {
    "DefaultProvider": "claude",
    "Providers": {
      "claude": {
        "Enabled": true,
        "Command": "",
        "Arguments": "",
        "ExtraFlags": [],
        "TimeoutSeconds": 60
      },
      "codex": {
        "Enabled": true,
        "Command": "",
        "Arguments": "",
        "ExtraFlags": [],
        "TimeoutSeconds": 60
      },
      "gemini": {
        "Enabled": true,
        "Command": "",
        "Arguments": "",
        "ExtraFlags": [],
        "TimeoutSeconds": 60
      },
      "qwen": {
        "Enabled": true,
        "Command": "",
        "Arguments": "",
        "ExtraFlags": [],
        "TimeoutSeconds": 60
      },
      "kilocode": {
        "Enabled": true,
        "Command": "",
        "Arguments": "",
        "ExtraFlags": [],
        "TimeoutSeconds": 60
      },
      "opencode": {
        "Enabled": true,
        "Command": "",
        "Arguments": "",
        "ExtraFlags": [],
        "TimeoutSeconds": 60
      },
      "lmstudio": {
        "Enabled": true,
        "Command": "",
        "Arguments": "",
        "ExtraFlags": [],
        "TimeoutSeconds": 60
      }
    },
    "LmStudio": {
      "Enabled": true,
      "BaseUrl": "http://192.168.88.9:1234/v1",
      "ApiKeyFile": "secrets/lmstudio_api_key.txt",
      "ApiKey": "",
      "ChatModel": "neuraldaredevil-8b-abliterated",
      "EmbeddingModel": "text-embedding-3-small",
      "MaxInputTokens": 8192
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File",
      "Serilog.Enrichers.Environment"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] ({CorrelationId}) {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/tracker-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7,
          "shared": true,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] ({CorrelationId}) {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
